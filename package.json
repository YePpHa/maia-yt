{
  "name": "project-maia",
  "version": "0.2.2",
  "description": "Enhances the user experience on YouTube by providing useful features.",
  "keywords": [
    "YouTube",
    "userscript",
    "extension",
    "chrome",
    "firefox",
    "enhance"
  ],
  "main": "webpack.config.js",
  "scripts": {
    "build": "npm run build:userscript && npm run build:webextension",
    "build:userscript": "cross-env NODE_ENV=production webpack --mode production --config config/userscript.webpack.config.js",
    "build:webextension": "npm run build:webextension:manifest && npm run build:webextension:background && npm run build:webextension:content && npm run build:webextension:inject && npm run build:webextension:zip",
    "build:webextension:manifest": "node ./build/webextension.manifest.js",
    "build:webextension:background": "cross-env NODE_ENV=production webpack --mode production --config config/webextension.background.webpack.config.js",
    "build:webextension:content": "cross-env NODE_ENV=production webpack --mode production --config config/webextension.content.webpack.config.js",
    "build:webextension:inject": "cross-env NODE_ENV=production webpack --mode production --config config/webextension.inject.webpack.config.js",
    "build:webextension:zip": "node ./build/webextension.zip.js",
    "dev:userscript": "webpack --config --mode development config/userscript.webpack.config.js",
    "dev:webextension": "npm run build:webextension:manifest && npm run dev:webextension:background && npm run dev:webextension:content && npm run dev:webextension:inject",
    "dev:webextension:background": "webpack --mode development --config config/webextension.background.webpack.config.js",
    "dev:webextension:content": "webpack --mode development --config config/webextension.content.webpack.config.js",
    "dev:webextension:inject": "webpack --mode development --config config/webextension.inject.webpack.config.js",
    "test": "karma start"
  },
  "author": {
    "name": "Jeppe Rune Mortensen",
    "email": "jepperm@gmail.com",
    "url": "https://github.com/YePpHa"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YePpHa/maia-yt.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-classes": "^7.10.4",
    "@babel/plugin-transform-object-assign": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@types/greasemonkey": "^4.0.0",
    "@types/i18next": "^13.0.0",
    "@types/jasmine": "^3.5.14",
    "@types/node": "^14.11.1",
    "@types/sprintf-js": "^1.1.2",
    "@types/uuid": "^8.3.0",
    "archiver": "^5.0.2",
    "async": "^3.2.2",
    "babel-loader": "^8.1.0",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^3.6.0",
    "karma": "^5.2.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-sauce-launcher": "^4.1.5",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^4.0.2",
    "memory-fs": "^0.5.0",
    "mkdirp": "^1.0.4",
    "node-sass": "^4.14.1",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.4",
    "typescript": "^4.0.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "wrapper-webpack-plugin": "^2.1.0"
  },
  "dependencies": {
    "i18next": "^19.7.0",
    "i18next-resource-store-loader": "^0.1.2",
    "immutable": "^4.0.0-rc.12",
    "inversify": "^5.0.1",
    "preact": "^10.4.8",
    "preact-material-components": "^1.6.1",
    "preact-router": "^3.2.1",
    "reflect-metadata": "^0.1.13",
    "sprintf-js": "^1.1.2",
    "uuid": "^8.3.0",
    "webextension-polyfill": "^0.6.0",
    "webpack-merge": "^5.1.4"
  }
}
